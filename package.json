{
  "name": "chromatique-perfume",
  "version": "1.0.0",
  "description": "Chromatique - Premium Perfume Brand Website with SCSS, GSAP & Swiper",
  "scripts": {
    "sass": "sass assets/scss/style.scss:assets/css/style.css --watch",
    "sass:build": "sass assets/scss/style.scss:assets/css/style.css --style=compressed",
    "sass:dev": "sass assets/scss/style.scss:assets/css/style.css --watch --style=expanded",
    "build": "npm run sass:build"
  },
  "keywords": [
    "perfume",
    "luxury",
    "brand",
    "scss",
    "gsap",
    "swiper"
  ],
  "author": "Chromatique Team",
  "license": "MIT",
  "devDependencies": {
    "sass": "^1.77.0"
  }
}
